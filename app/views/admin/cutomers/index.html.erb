<h5>会員一覧</h5>
<% @customers.each do |customer|  %>
<table class="table table-hover table-inverse">
  <thead>
    <tr>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= customer.id %></td>
      <td><%= customer.name %></td>
      <td><%= customer.email %></td>
      <td><%= customer.is_deleted %></td>
    </tr>
  </tbody>
</table>