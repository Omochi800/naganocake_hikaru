<div class="col-md-2">
   <p class="bg-light">ジャンル編集</p>
</div>
<%= form_with model: @genre,url: admin_genre_path(@genre),method: :patch do |f| %>
<div class="container">
  <div class="field">
        <%= f.label:ジャンル名 %>
        <%= f.text_field :name %>
        <%= f.submit '変更を保存',class: 'btn btn-sm btn-success' %>
  </div>
</div>
<% end %>